<?xml version="1.0"?>
<extension key="com.joineryhq.noemptyreportmail" type="module">
  <file>noemptyreportmail</file>
  <name>No Empty Report Mail</name>
  <description>Prevent emailing of reports that contain no rows in the output.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/JoineryHQ/com.joineryhq.noemptyreportmail</url>
    <url desc="Documentation">https://github.com/JoineryHQ/com.joineryhq.noemptyreportmail</url>
    <url desc="Support">https://github.com/JoineryHQ/com.joineryhq.noemptyreportmail/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2023-10-01</releaseDate>
  <version>1.0.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>Developed by Joinery for the CiviCRM community.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Noemptyreportmail</namespace>
    <format>23.02.1</format>
    <angularModule>crmNoemptyreportmail</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
